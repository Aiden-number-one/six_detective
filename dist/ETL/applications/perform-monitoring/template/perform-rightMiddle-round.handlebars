 {{#if flag}}
<div class="portlet-body" style="overflow: hidden;display:block">
    <div class="circleChart-box" style="min-height:120px">
    </div>
</div>
{{else}} {{#each this}}
<div class="portlet-body" style="overflow: hidden;display:inline-block">
    <div class="circleChart-box" style="min-height:120px">
        <div class="num-block" style="position:absolute;z-index:999;width:100%">
            <div class="circleChart-title circleChart-title-one" title="执行成功：{{successNum}}个" style="color:#17C4BB">{{successNum}}</div>
            <div class="circleChart-title circleChart-title-two" title="执行失败：{{errorNum}}个" style="color:#ed6b75">{{errorNum}}</div>
        </div>

        <div class="circleChart" id="circle{{@index}}"></div>
        <div class="circleChart-text">
            <div class="circle-overflow text-overflow" batchNo="{{batchNo}}" zxsj="{{zxsj}}" jobName="{{nodeName}}" jobId="{{jobId}}" executeFlag="{{executeFlag}}" jobNo="{{memberNo}}" errorNum="{{errorNum}}" startTime="{{startTime}}" endTime="{{endTime}}" executeTypeName="{{executeTypeName}}" title="{{batchNo}}">{{batchNo}}</div>
        </div>
    </div>
</div>
{{/each}} {{/if}}