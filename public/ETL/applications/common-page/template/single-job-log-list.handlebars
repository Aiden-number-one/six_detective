{{#if flag}}
<tr>
    <td colspan="18" class="text-center">暂无数据</td>
</tr>
{{else}} {{#each this}}

<tr>
    <td class="t-c">{{ROWNUM_}}</td>
    <td class="t-l"> {{monitorLevel}} </td>
    <td class="t-c"> {{orderNo}}</td>
    <td class="t-c"> {{jobName}}</td>
    <td class="t-c"> {{nodeName}} </td>
    <td class="t-c"> {{memberTypeName}} </td>
    <td class="t-c"> {{dateTimeFormatHelper startTime}} </td>
    <td class="t-c"> {{dateTimeFormatHelper endTime}} </td>
    <td class="t-r"> {{formatDuration duration}} </td>
    <td class="t-r"> {{successNum}} </td>
    <td class="t-r"> {{errorNum}} </td>
    <td class="t-c"> <span style="color: {{executeFlagFormat executeFlag}}">●</span> {{executeFlagName}} </td>
    <td class="t-l" style="max-width:160px;">
        <div style="width: 160px;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;cursor:pointer;color:#29b2e6" class="executeMsg">{{executeMsg}}</div>
    </td>
</tr>
{{/each}} {{/if}}