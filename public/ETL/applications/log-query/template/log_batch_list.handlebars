{{#if flag}}
<tr>
    <td colspan="18" class="text-center">No Data</td>
</tr>
{{else}} {{#each this}}

<tr>
    <td class="t-c">{{ROWNUM_}}</td>
    <td class="t-l"> {{executorId}} </td>
    <td class="t-l"> {{jobName}}</td>
    {{!-- <td class="t-c"> {{batchNo}}</td> --}}
    <td class="t-c"> {{executeTypeName}} </td>
    <td class="t-c"> {{dateTimeFormatHelper startTime}} </td>
    <td class="t-c"> {{dateTimeFormatHelper endTime}} </td>
    <td class="t-c"> {{formatDuration duration}} </td>
    <td class="t-r"> {{successNum}} </td>
    <td class="t-r"> {{errorNum}} </td>
    <td class="t-c"> <span style="color: {{executeFlagFormat executeFlag}}">‚óè</span> {{executeFlagName}} </td>
    <td class="t-l" style="width:160px;">
        <div style="max-width: 160px;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;cursor:pointer; color: #29b2e6" class="executeMsg">{{executeMsg}}</div>
    </td>
    <td class="t-c">
        <a href="/index.html#common-page" data-toggle="link" data-linkto="single-job-log" data-params='{"jobId":"{{jobId}}","batchNo":"{{batchNo}}"}'
            data-type="none">Detail</a>
    </td>
</tr>
{{/each}} {{/if}}