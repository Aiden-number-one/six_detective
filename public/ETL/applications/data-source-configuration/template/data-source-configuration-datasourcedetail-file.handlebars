{{#if flag}}
<tr>
  <td colspan="20" class="t-c">No Data</td>
</tr>
{{else}} {{#each this}}
<tr>
    <td class="t-c" data-tableId={{tableId}}>
        <input type="checkbox" class="">
    </td>
    <td>{{calcIndex @index}}</td>
    <td>{{fileName}}</td>
    <td>{{fileType}}</td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td class="t-c" fileUrl='{{fileUrl}}' fileType='{{fileType}}' fileName='{{fileName}}' connectionId='{{connectionId}}'>
        <a id="dsc-modal-check-file" href="javascript:;" data-lang="PREVIEW">PREVIEW</a>
        <a id="dsc-modal-delete-file" href="javascript:;" data-lang="DELETE">DELETE</a>
    </td>

</tr>
{{/each}} {{/if}}
<script src="/assets/js/lang.js"></script>